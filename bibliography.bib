  keywords  = {Quantum Physics, Statistics - Machine Learning},
  annote    = {Comment: 26 pages, 9 figures - Version 2 emphasises focus on supervised learning, adds more references to existing literature, deletes section on state discrimination due to a technical error, and updates the comparison between kernel-based and variational training},
  file      = {PDF:/home/elisa/Zotero/storage/N86ZTYDG/Schuld - 2021 - Supervised quantum machine learning models are kernel methods.pdf:application/pdf}
}

@article{TransmonPaper,
