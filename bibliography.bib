  note     = {arXiv:cond-mat/0703002},
  keywords = {Condensed Matter - Mesoscale and Nanoscale Physics, Quantum Physics},
  pages    = {042319},
  annote   = {Comment: 21 pages, 12 figures; title changed, small changes in the text, additional figure},
